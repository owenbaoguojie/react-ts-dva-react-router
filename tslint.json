{
    "extends": ["tslint-react", "tslint-config-airbnb"],
    "rules": {
        "import-name": false,
        "variable-name": false,
        "jsx-boolean-value": false,
        "function-name": false,
        "object-shorthand-properties-first": false,
        "jsx-no-multiline-js": false,
        "jsx-no-lambda": false,
        "brace-style": [true, "stroustrup"],
        "ter-prefer-arrow-callback": false
    }
}
